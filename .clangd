CompileFlags:
  Add: [
    "-std=c++98",
    "-Wall",
    "-Wextra",
    "-Werror",
    "-I.",
    "-xc++",
    "-isystem",
    "-isystem",
    "/usr/local/include",
    "-isystem",
    "/usr/lib/llvm-12/lib/clang/12.0.1/include",
    "-isystem",
    "/usr/include/x86_64-linux-gnu",
    "-isystem",
    "/usr/include",
    "-isystem",
    "/usr/include/c++/11"
  ]
  Remove: ["-W*"]
Diagnostics:
  Suppress: [
    "pp_including_mainfile_in_preamble",
    "incomplete-type"
  ]
InlayHints:
  Enabled: false
  
Index:
  StandardLibrary: true
